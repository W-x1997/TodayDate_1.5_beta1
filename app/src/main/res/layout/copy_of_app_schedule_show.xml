              style="@style/show_tv1">
          </TextView>
          <LinearLayout
              android:layout_width="fill_parent"
              android:layout_height="wrap_content"
              android:orientation="vertical"
              >
              <TextView
                  android:id="@+id/show_tv5_2"
                  android:layout_width="fill_parent"
                  android:layout_height="wrap_content"
                  android:text="大学生心理健康教育"
                  style="@style/show_tv2">
              </TextView>
              <TextView
                  android:id="@+id/show_tv5_3"
                  android:layout_width="fill_parent"
                  android:layout_height="wrap_content"
                  android:text="教四 403"
                  style="@style/show_tv3"
                  
                  >
              </TextView>              
          </LinearLayout>
      </TableRow>

</TableLayout>
</LinearLayout>
